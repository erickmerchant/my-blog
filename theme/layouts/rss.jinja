<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
	<channel>
		<title>{{entry.title}}</title>
		<link>{{site.host + "/" + entry.category + "/" + entry.slug + "/feed.rss"}}</link>
		<copyright>Â© {{current_date() | format_date("%Y")}} {{site.author}}</copyright>
		{% for (e, tags) in feed %}
			<item>
				<title>{% if e.title %}{{e.title}}{% else %}Untitled{% endif %}</title>
				<link>{{site.host + "/" + e.category + "/" + e.slug + "/"}}</link>
				<pubDate>{{e.date | format_date("%a, %d %h %Y")}} 00:00:00 +0000</pubDate>
				<description>
					<![CDATA[{{e.description}}]]>
				</description>
			</item>
		{% endfor %}
	</channel>
</rss>
